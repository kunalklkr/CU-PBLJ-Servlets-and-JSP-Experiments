CREATE DATABASE IF NOT EXISTS company;

USE company;

CREATE TABLE Employee (
    EmpID INT PRIMARY KEY,
    Name VARCHAR(100) NOT NULL,
    Salary DECIMAL(10, 2)
);

INSERT INTO Employee (EmpID, Name, Salary) VALUES
